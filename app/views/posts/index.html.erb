<div class="row">
	<div class="col-sm-3"></div>
	<div class="col-sm-6">
	<h1 class="title">POSTS</h1><br>
<% @post.each do |post| %>
<div class="post_wrapper">
	<h2 class="title">
		<%= link_to post.title, post%>
	</h2>
	<i>
		<%= post.created_at.strftime("%B %d, %Y") %>
	</i>
</div>

<% end %>
</div>
<div class="col-sm-3"></div>
</div>